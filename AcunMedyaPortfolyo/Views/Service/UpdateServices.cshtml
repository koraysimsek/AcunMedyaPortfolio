@model AcunMedyaPortfolyo.Models.Services
@{
    ViewBag.Title = "UpdateServices";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>UpdateServices</h2>

<form method="post">
    @Html.HiddenFor(x => x.ServicesID)
    <label>Açıklama</label>
    <br />
    @Html.TextBoxFor(x => x.Description)
    <br />
    <label>Başlık</label>
    <br />
    @Html.TextBoxFor(x => x.Title)
    <br />
    <label>İkon Linki</label>
    <br />
    @Html.TextBoxFor(x => x.IconURL)
    <br />
    <label>Açıklama2</label>
    <br />
    @Html.TextBoxFor(x => x.Description2)
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>